SELECT AUDIT_TRAIL.ACTION_NAME, AUDIT_TRAIL.DBUSERNAME, AUDIT_TRAIL.OBJECT_SCHEMA,AUDIT_TRAIL.OBJECT_NAME, AUDIT_TRAIL.EVENT_TIMESTAMP,
AUDIT_TRAIL_SYSTEM_PRIVILEGE_USED,AUDIT_TRAIL.UNIFIED_AUDIT_POLICIES,
AUDIT_TRAIL.USERHOST,AUDIT_TRAIL.OS_USERNAME, AUDIT_TRAIL.CLIENTE_PROGRAM_NAME,
AUDIT_TRAIL.APPLICATION_CONTEXTS, DBMS_LOG.SUBSTR(AUDIT_TRAIL.SQL_TEXT,200,1) AS SQL_TEXT
FROM SYS.UNIFIED_AUDIT_TRAIL AUDIT_TRAIL WHERE TO_CHAR(EVENT_TIMESTAMP,'YY')='17' 
ORDER BY AUDIT_TRAIL.EVENT_TIMESTAMP

select sql_text,event_timestamp,object_name from SYS.UNIFIED_AUDIT_TRAIL where action_name='ALTER USER' order by event_timestamp desc
select * from SYS.UNIFIED_AUDIT_TRAIL
